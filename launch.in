#!/bin/bash
ROOT_DIR=@CMAKE_SOURCE_DIR@
GAZEBO_MODEL_PATH=@GAZEBO_MODEL_PATH@
GAZEBO_SHARE_PATH=$GAZEBO_MODEL_PATH/../..
source $GAZEBO_SHARE_PATH/gazebo/setup.sh
export GAZEBO_MODEL_PATH=$ROOT_DIR/Models:$GAZEBO_MODEL_PATH
export GAZEBO_RESOURCE_PATH=$ROOT_DIR/Worlds:$GAZEBO_RESOURCE_PATH
export LD_LIBRARY_PATH=$ROOT_DIR/Build:$LD_LIBRARY_PATH
export DYLD_LIBRARY_PATH=$LD_LIBRARY_PATH:$DYLD_LIBRARY_PATH

gazebo inverted_pendulum_cart.world

